package collections;

import java.util.ArrayList;

public class ArrayListPractice {
	


	public static void arrayListMethod() {
 
		ArrayList<String> list = new ArrayList<String>();
		list.add("User1");
		list.add("User2");
		list.add(2,"User4");
		list.add("User3");
		
		//System.out.println(list.toString());
		System.out.println("list : " +list);
		
		
	}

}
